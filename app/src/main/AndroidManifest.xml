<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.example.administrator.testarcface"
          xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- camera -->
    <uses-permission android:name="android.permission.CAMERA" />
    <!-- sdcard -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <!-- wifi internet -->
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name="com.example.administrator.testarcface.Application"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:largeHeap="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.administrator.testarcface.PermissionAcitivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.example.administrator.testarcface.regist.RegisterActivity">
        </activity>
        <activity android:name="com.example.administrator.testarcface.DetecterActivity">
        </activity>
        <activity android:name="com.example.administrator.testarcface.MainActivity">
        </activity>
        <activity android:name="com.example.administrator.testarcface.photo.GallaryActivity">
        </activity>
        <activity android:name=".photo.ImageActivity">
        </activity>
    </application>

</manifest>